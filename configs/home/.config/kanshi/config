profile twomonitors {
	output eDP-1 disable
	output HDMI-A-1 mode 1920x1080@71.910Hz position 1536,0 enable
}
profile laptoponly {
	output eDP-1 scale 1.25 position 0,0 enable
}

profile home_three_monitors {
	output eDP-1 disable
	output HDMI-A-1 scale 1.25 mode 2560x1440@144Hz position 0,0
	output DP-1 scale 1.5 mode 3840x2160@144Hz position 2048,0
}

